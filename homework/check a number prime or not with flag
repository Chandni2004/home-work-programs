#include <stdio.h>
#include <math.h>
int main()
{
    enum bool{false,true};
    int i,n;
    enum bool FLAG=true;
    printf("Enter a number: ");
    scanf("%d",&n);
    for(i=2;i<=n/2;i++)
    {
        if(n%i==0)
        {
            FLAG=false;
            break;
        }
    }
    if(FLAG==true)     
    printf("Prime number");
    else
    printf("Not Prime Number");
}
