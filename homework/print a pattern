#include<stdio.h>
#include<stdlib.h>
int main()
{
    int n,i,j,k,l;
    printf("Enter the number of Rows: ");
    scanf("%d",&n);
    for(i=n;i>=1;i--)
    {
        for(j=1;j<=i;j++)
        printf("%c",j+97-1);
        for(k=1;k<=2*(n-i)-1;k++)
        printf(" ");
        for(l=i;l>=1;l--)
        {
            if(l==n)
            continue;   
            printf("%c",l+97-1);
        }
        printf("\n");
    }
}
